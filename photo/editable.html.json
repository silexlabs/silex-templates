tate(45deg) translate(7px, -4px);\n}\n\n.hamburger-menu_1647532519697_617.open .hamburger-btn span:nth-last-child(2) {\n    opacity: 0;\n}\n\n.hamburger-menu_1647532519697_617 .hamburger-btn span:nth-child(3) {\n    transform-origin: -11px 4px;\n}\n.hamburger-menu_1647532519697_617.open .hamburger-btn span:nth-child(3) {\n    transform: rotate(-45deg) translate(0, 12px);\n}\n\n/* themes */\n\n\n</style>\n<!-- inspired by https://codepen.io/erikterwan/pen/EVzeRP -->\n<div class=\"hamburger-menu_1647532519697_617 closed\">\n    <div class=\"hamburger-btn \">\n      <span></span>\n      <span></span>\n      <span></span>\n    </div>\n    <div class=\"nav-container\">\n      <div class=\"prevent-scale\">\n        <nav class=\"\">\n            <ul>\n            \n                <li><a\n                    \n                        href=\"http://localhost:6805/ce/fs/get/Documents/silex-templates/photo/#home\"\n                    \n                >Home</a></li>\n            \n                <li><a\n                    \n                        href=\"http://localhost:6805/ce/fs/get/Documents/silex-templates/photo/#portfolio\"\n                    \n                >Portfolio</a></li>\n            \n                <li><a\n                    \n                        href=\"http://localhost:6805/ce/fs/get/Documents/silex-templates/photo/#about\"\n                    \n                >About</a></li>\n            \n                <li><a\n                    \n                        href=\"http://localhost:6805/ce/fs/get/Documents/silex-templates/photo/#contact\"\n                    \n                >Contact</a></li>\n            \n            </ul>\n        </nav>\n      </div>\n  </div>\n</div>\n<script>\n(function() {\n  var isOpen = false;\n  function close() {\n    isOpen = false;\n    document.querySelector('.hamburger-menu_1647532519697_617').classList.remove('open');\n    document.querySelector('.hamburger-menu_1647532519697_617').classList.add('closed');\n  }\n  function open() {\n    isOpen = true;\n    document.querySelector('.hamburger-menu_1647532519697_617').classList.add('open');\n    document.querySelector('.hamburger-menu_1647532519697_617').classList.remove('closed');\n  }\n  function toggle() {\n    if(isOpen) close();\n    else open();\n  }\n  function scrollOrResize() {\n    close();\n    move();\n  }\n  var silexElement = document.querySelector('.hamburger-menu_1647532519697_617').closest('.editable-style');\n  function move() {\n    var data = window.silex\n    var rect = silexElement.getBoundingClientRect();\n    var offset = {\n      top: rect.top + data.scrollTop,\n      left: rect.left + data.scrollLeft,\n    };\n    var container = document.querySelector('.hamburger-menu_1647532519697_617 .nav-container');\n    container.style.transform = 'translate(' + (data.scrollLeft - (offset.left/data.scale)) + 'px, ' + (data.scrollTop - (offset.top/data.scale)) + 'px)';\n  }\n  document.querySelector('.hamburger-menu_1647532519697_617 .hamburger-btn').addEventListener('click', function() { toggle(); });\n  document.addEventListener('resize', scrollOrResize);\n  document.addEventListener('scroll', scrollOrResize);\n  scrollOrResize()\n  setTimeout(() => scrollOrResize(), 0)\n})();\n</script>\n","tagName":"DIV"},{"id":"silex-id-1552779510769-9","pageNames":[],"classList":[],"type":"html-element","isSectionContent":false,"title":"","alt":null,"children":[],"link":null,"enableEdit":true,"enableDrag":true,"enableDrop":false,"enableResize":{"top":true,"bottom":true,"left":true,"right":true},"selected":false,"useMinHeight":true,"visibility":{"desktop":true,"mobile":true},"style":{"desktop":{"width":"227px","background-color":"transparent","top":"12px","left":"401px","height":"20px","position":"absolute"},"mobile":{"left":"72px","top":"21px","width":"359px","height":"20px"}},"data":{"component":null},"innerHtml":"","tagName":"DIV"},{"id":"silex-id-1521479714244-27","pageNames":[],"classList":["fixed","full-width"],"typ